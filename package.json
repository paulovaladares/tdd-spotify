{
  "name": "tdd-spotify",
  "version": "1.0.0",
  "description": "TDD Spotify stydy",
  "main": "src/index.js",
  "scripts": {
    "clear": "rimraf lib",
    "build": "npm run clear && ./node_modules/.bin/babel --out-dir lib src",
    "build:watch": "npm run build -- --watch",
    "lint": "./node_modules/.bin/eslint src/*.js",
    "test": "./node_modules/.bin/mocha tests/**/*.spec.js --require babel-register",
    "test:tdd": "./node_modules/.bin/mocha tests/**/*.spec.js --require babel-register --watch",
    "test:coverage": "nyc npm test"
  },
  "nyc": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paulovaladares/tdd-spotify.git"
  },
  "keywords": [
    "js",
    "tdd",
    "library"
  ],
  "author": "Paulo Valadares <contato@paulovaladares.com.br>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/paulovaladares/tdd-spotify/issues"
  },
  "homepage": "https://github.com/paulovaladares/tdd-spotify#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-node": "0.0.1-security",
    "babel-preset-env": "^1.3.2",
    "babel-register": "^6.24.0",
    "chai": "^3.5.0",
    "eslint": "^4.0.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.6.1",
    "husky": "^0.11.9",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "rimraf": "^2.6.1",
    "sinon": "^9.2.3",
    "sinon-chai": "^3.5.0"
  },
  "dependencies": {
    "node-fetch": "^2.6.1"
  }
}
